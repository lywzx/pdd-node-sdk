{
  "name": "pdd-node-sdk",
  "version": "0.0.11",
  "description": "pinduoduo open platform node sdk",
  "main": "dist/index.js",
  "types": "dist/@types/index.d.ts",
  "scripts": {
    "build": "tsc",
    "lint": "eslint src --ext .ts --fix",
    "format": "prettier --write \"+(src|scripts)/**/*.ts\"",
    "test": "mocha --require ts-node/register test/*.ts test/**/*.ts",
    "report-coverage": "codecov",
    "generate": "cross-env NODE_ICU_DATA=node_modules/full-icu ts-node -r tsconfig-paths/register ./scripts/index.ts",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "engines": {
    "node": ">=8.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lywzx/pdd-node-sdk.git"
  },
  "keywords": [
    "pdd",
    "pinduoduo",
    "duoduoke",
    "pdd-sdk",
    "sdk",
    "拼多多",
    "多多客",
    "拼多多SDK"
  ],
  "author": "liuyang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lywzx/pdd-node-sdk/issues"
  },
  "homepage": "https://github.com/lywzx/pdd-node-sdk#readme",
  "devDependencies": {
    "@types/async": "^3.2.0",
    "@types/chai": "^4.2.11",
    "@types/crypto-js": "^3.1.44",
    "@types/debug": "^4.1.5",
    "@types/lodash": "^4.14.149",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "full-icu": "^1.3.1",
    "install": "^0.13.0",
    "mocha": "^7.1.1",
    "prettier": "^1.19.1",
    "tiny-pinyin": "^1.3.0",
    "ts-node": "^8.8.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "async": "^3.2.0",
    "axios": "^0.19.2",
    "crypto-js": "^4.0.0",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "snyk": "^1.316.1"
  },
  "snyk": true
}